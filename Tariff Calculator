a=int(input())
if(a>12):
    print('Invalid Input')
if(a==1 or a==2 or a==3 or a==7 or a==8 or a==9 or a==10):
    r=float(input())
    d=int(input())
    Tariff=(r*d)
    print(f'Hotel Tariff:Rs.''%.2f'%Tariff)
if(a==4 or a==5 or a==6 or a==11 or a==12):
    r=float(input())
    d=int(input())
    Tariff=(r+r*(20/100))*d
    print(f'Hotel Tariff:Rs.''%.2f'%Tariff)
